<%= form_for @profile do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :age %>
  <%= f.text_field :age %>
  <%= f.label :image %>
  <%= f.file_field :image %>
  <%= f.label :bio %>
  <%= f.text_field :bio %>
  <%= f.label :gender %>
  <%= f.select :gender,%w{Male Female}, required: true, placeholder: 'Gender', class: 'form-control' %>

  <%= f.submit %>
<% end %>